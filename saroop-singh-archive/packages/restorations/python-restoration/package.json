{
  "name": "saroop-singh-restorations-api",
  "version": "1.0.0",
  "description": "AI-powered vintage photograph restoration API service for the Saroop Singh Archive",
  "main": "api/process.py",
  "scripts": {
    "dev": "vercel dev",
    "deploy": "./deploy.sh",
    "test": "python -m pytest tests/",
    "lint": "python -m flake8 . --max-line-length=100",
    "format": "python -m black .",
    "install-deps": "pip install -r requirements.txt"
  },
  "keywords": [
    "ai",
    "image-restoration",
    "vintage-photos",
    "gemini-ai",
    "airtable",
    "vercel",
    "serverless",
    "photo-restoration",
    "historical-archive"
  ],
  "author": "Saroop Singh Archive Project",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yourusername/saroop-singh-archive.git"
  },
  "bugs": {
    "url": "https://github.com/yourusername/saroop-singh-archive/issues"
  },
  "homepage": "https://github.com/yourusername/saroop-singh-archive#readme",
  "engines": {
    "node": ">=18.0.0",
    "python": ">=3.9"
  },
  "vercel": {
    "functions": {
      "api/**/*.py": {
        "runtime": "python3.9"
      }
    }
  },
  "devDependencies": {
    "@vercel/python": "^4.0.0"
  }
}