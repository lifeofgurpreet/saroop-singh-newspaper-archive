{
  "version": 2,
  "builds": [
    {
      "src": "packages/web/package.json",
      "use": "@vercel/next",
      "config": {
        "distDir": "packages/web/.next"
      }
    }
  ],
  "routes": [
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "dest": "packages/web/$1"
    }
  ],
  "outputDirectory": "packages/web/.next"
}